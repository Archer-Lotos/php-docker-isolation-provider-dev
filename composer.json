{
    "type": "project",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "lotos2021/deepclient-test-php",
                "version": "0.0.1",
                "source": {
                    "url": "https://github.com/Lotos2021/deepclient-test-php.git",
                    "type": "git",
                    "reference":"main"
                }
            }
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "lotos2021/deepclient-test-php": "0.0.1",
        "slim/slim": "4.12.0",
        "vlucas/phpdotenv": "^5.5",
        "webonyx/graphql-php": "^15.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
