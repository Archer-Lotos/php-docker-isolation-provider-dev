{
    "type": "project",
    "require": {
        "php": "^7.4",
        "monolog/monolog": "2.9.1",
        "slim/psr7": "1.6.1",
        "slim/slim": "4.12.0",
        "vlucas/phpdotenv": "5.5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
